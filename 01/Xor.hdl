/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=w1);
    Not(in=b, out=w2);
    Nand(a=a, b=w2, out=w3);
    Nand(a=w1, b=b, out=w4);
    Nand(a=w3, b=w4, out=out);
}
